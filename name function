= (x) => if Text.EndsWith(x,">") then 0 else if Text.StartsWith(x, "CO ") or Text.StartsWith(x, "C\") or Text.StartsWith(x, "C/") or Text.StartsWith(x, "C ") or Text.StartsWith(x, "ATT") or Text.StartsWith(x, """") or Text.StartsWith(x, "THE ") or Text.StartsWith(x, "'") or Text.EndsWith(x, "ROAD") or Text.EndsWith(x, " FARM") or Text.EndsWith(x, " ESTATE") or Text.EndsWith(x, " TOWER") or Text.EndsWith(x, " STREET") or Text.EndsWith(x, " LANE") or Text.EndsWith(x, " CENTRE") or Text.EndsWith(x, " BUILDING") or Text.PositionOfAny(x,{"0".."9"})>=0 then 1 else 0
