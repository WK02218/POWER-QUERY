= (x)=> if Text.StartsWith(x,"<") or Text.StartsWith(x,"MR ") or Text.StartsWith(x,"MS ") or Text.StartsWith(x,"MRS ") or Text.StartsWith(x,"MISS ") or Text.StartsWith(x,"DR ") or Text.StartsWith(x,"M/S ") or (not Text.StartsWith(x,"C\") and Text.EndsWith(x," LIMITED")) or (not Text.StartsWith(x,"C\") and Text.EndsWith(x," LTD")) or (not Text.StartsWith(x,"C\") and Text.EndsWith(x,"CO LTD")) or Text.Contains(x," A/C") then 1 else 0
